{
  "name": "indentation-select",
  "displayName": "indentation-select",
  "description": "Select everything at the same indentation level",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:indentation-select.select"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings":[
      {
        "command": "indentation-select.select",
        "key": "cmd+shift+j"
      }
    ],
    "commands": [
      {
        "command": "indentation-select.select",
        "title": "Select text at same indentation level"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "prettier": {
    "tabWidth": 2,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
